[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = LeeSF03
	email = shuenfeilee@gmail.com
[http]
	version = HTTP/2
[core]
	pager = delta

[interactive]
	diffFilter = delta --color-only --feature=interactive
	
[merge]
	conflictstyle = diff3

[diff]
	colorMoved = default

[include]
	path = C:/Users/shuen/.config/delta/themes/catppuccin.gitconfig

[delta]
	features = catppuccin-mocha
	line-numbers = true
	side-by-side = true
	navigate = true

[delta "decorations"]
	hunk-header-style = file line-number

[gui]
	recentrepo = C:/Users/shuen/.config
[init]
	defaultBranch = main

